platform :ios, '13.0'

target 'CourseGrab' do
  use_frameworks!

  # Pods for CourseGrab
  pod 'Firebase/Auth'
  pod 'FutureNova', :git => 'https://github.com/cuappdev/ios-networking.git'
  pod 'GoogleSignIn'
  pod 'SnapKit'
  pod 'SPPermissions/Notification'
  pod 'Tactile'
  pod 'Wormholy', :configurations => ['Debug']

end
